[begin] (ScrudgeWare)
  [exec] (Run)     {bbrun -w}
  [exec] (Eterm)     {Eterm}
  [exec] (Users)     {gnuserman}
  [exec] (Gkrellm)   {gkrellm}
  [exec] (Edit Menu) {gvim /usr/local/share/hackedbox/menu}
  [exec] (xScreenSaver) {xscreensaver-demo}

  [nop] ()

  [submenu] (Office Tools)
    [exec] (AbiWord)  {abiword}
    [exec] (Gnumeric) {gnumeric}
    [exec] (Gvim)     {gvim}
    [exec] (Samba)    {xfsamba}
  [end]

  [submenu] (Development)
    [exec] (Anjuta)   {anjuta}
    [exec] (BlueFish) {bluefish}
  [end]
 
  [submenu] (Remote Terms)
    [exec] (FTP scrudgeware)         {Eterm -e /usr/local/bin/ftp ftp.scrudgeware.org}
    [exec] (SSH Example)         {Eterm -e /usr/local/bin/ssh you@example.com}
  [end]

  [submenu] (Multimedia)
    [exec] (The GIMP)  {gimp}
    [exec] (Xmms)      {xmms}
    [exec] (Mixer)     {opmixer}
    [exec] (CD Play)   {gcd}
    [exec] (Giram)  {giram}
  [end]

  [submenu] (Internet)
    [exec] (SkipStone)     {skipstone}
    [exec] (Mail)     {sylpheed}
    [exec] (ftp) {guiftp}
    [exec] (Gnutella) {gtk-gnutella}
    [exec] (Mozilla)  {/usr/local/encap/mozilla/mozilla}
    [exec] (Composer) {/usr/local/encap/mozilla/mozilla -edit}
  [end]

  [submenu] (X Utilities)
    [exec] (Xkill) {xkill}
    [exec] (Xfontsel) {xfontsel}
    [exec] (Xman)     {xman}
    [exec] (Xcalc)    {xcalc}
    [exec] (Xload)    {xload}
  [end]
 
  [submenu] (Styles) {Choose a style...}
    [nop] ( *** HackedBox Styles *** )
    [stylesdir] (/usr/local/share/hackedbox/styles)
    [nop] ( *** Blackbox Styles *** )
    [stylesdir] (/usr/local/share/blackbox/styles)
  [end]

  [workspaces] (Workspace List)
  [config] (Configuration)

  [reconfig] (Reconfigure)
  
    [submenu] (Change WM?) {Change WM?}
        [restart] (Hackedbox)
		[restart] (blackbox) {blackbox}
		[restart] (openbox)  {openbox} 
		[restart] (fluxbox)  {fluxbox}
		[restart] (Window Maker) {wmaker}
		[restart] (Enlightenment) {enlightenment}
		[restart] (KWM) {kwm}
		[restart] (TWM) {twm}
	[end]

  [exit] (Exit)
[end]
